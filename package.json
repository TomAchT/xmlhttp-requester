{
  "name": "@flexio-oss/xmlhttp-requester",
  "version": "0.3.0-dev",
  "description": "Generic synchronous/asynchronous Http requester for flexio service clients",
  "main": "index.js",
  "module": "index.js",
  "repository": "https://github.com/flexiooss/xmlhttp-requester.git",
  "author": "Thomas Chatelain (https://github.com/TomAchT)",
  "license": "Apache-2.0",
  "dependencies": {
    "@flexio-corp/js-common-parent": "0.2.0-dev"
  },
  "devDependencies": {
    "code-altimeter-js": "https://github.com/flexiooss/code-altimeter-js.git",
    "@flexio-oss/hotballoon-test-dummies": "1.0.0-dev"
  },
  "hotballoon-shed": {
    "build": {
      "builder": "webpack4"
    },
    "test": {
      "tester": "code-altimeter-js",
      "path": "src/test"
    },
    "generate-sources": {
      "value-objects": {
        "extension": ".spec"
      }
    }
  }
}